#include <iostream>
using namespace std;
int prime(int n)
{
    int i,c=0;
    for(i=1;i<=n;i++)
    {
    if(n%i==0)
    {
        c++;
    }
    }
    return c;
}
int main()
{
    int n;
    cin>>n;
    if(prime(n)==2)
    {
        cout<<n<<" is a prime number";
    }
    else
    {
        cout<<n<<" is not a prime number";
    }
    
}
