#include<iostream>
using namespace std;
int max(int n)
{
    int a[n],i;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
    for(i=0;i<n;i++)
    {
        if(a[0]<a[i])
        {
            a[0]=a[i];
        }
    }return a[0];
}
int main()
{
    int n;
    cin>>n;
    cout<<"The maximum element in the array is "<<max(n);
    
}
