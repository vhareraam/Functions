#include<iostream>
using namespace std;
int ind(int n)
{
 	int i,p,k;
 	int a[n];
 	for(i=0;i<n;i++)
 	{
     	cin>>a[i];
 	}
 	cin>>p;
 	for(i=0;i<n;i++)
 	{
    	if(p==a[i]){
        	k=1;
        	//break;
    	}
   	 
 	}
 	if(k==1)
     	{
         	cout<<"Number present at index "<<i;
     	}
     	else
     	{
        	cout<<"The number is not present in the list";
     	}
     	return 0;
}

int main()
{
    int n;
    cin>>n;
    ind(n);
}
